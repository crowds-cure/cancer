{
  "name": "crowds-cure-cancer",
  "version": "0.1.0",
  "description": "Web app for crowd-sourcing annotations on cancer imaging datasets",
  "author": "crowds-cure",
  "repository": "https://github.com/crowds-cure/cancer",
  "license": "MIT",
  "dependencies": {
    "cornerstone-core": "^2.2.7",
    "cornerstone-math": "^0.1.6",
    "cornerstone-tools": "3.0.0-b.1106",
    "cornerstone-wado-image-loader": "^2.2.2",
    "dicom-parser": "^1.8.1",
    "dicomweb-client": "^0.3.2",
    "hammerjs": "^2.0.8",
    "husky": "^1.1.0",
    "lint-staged": "^8.0.4",
    "lodash.clonedeep": "^4.5.0",
    "oidc-client": "^1.5.3-beta.1",
    "pouchdb-browser": "^7.0.0",
    "pouchdb-find": "^7.0.0",
    "prettier": "^1.14.3",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-modal": "^3.6.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.1",
    "redux": "^4.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "scripts": {
    "copy:webworkers": "cp node_modules/cornerstone-wado-image-loader/dist/*.min.js* public",
    "start": "npm run copy:webworkers && react-scripts start",
    "build": "npm run copy:webworkers && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
